{
  "env": {
    "development": {
      "presets": [
        "stage-0",
        "env",
        "react"
      ],
      "plugins": [
        "babel-plugin-inline-import",
        [
          "transform-runtime",
          {
            "helpers": false,
            "polyfill": false,
            "regenerator": true,
            "moduleName": "babel-runtime"
          }
        ]
      ]
    }
  }
}